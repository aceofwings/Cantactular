#!/home/phantom-z/Documents/EVT/Cantactular/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'evtgateway==0.4.3','console_scripts','gateway'
__requires__ = 'evtgateway==0.4.3'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('evtgateway==0.4.3', 'console_scripts', 'gateway')()
    )
